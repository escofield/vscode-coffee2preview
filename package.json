{
  "name": "coffee2preview",
  "displayName": "Coffee2Preview",
  "description": "Preview CoffeeScript files compiled into javascript using the latest CoffeeScript.",
  "repository": {
    "url": "https://github.com/jhessin/vscode-coffee2preview.git"
  },
  "version": "0.0.0",
  "publisher": "jhessin",
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": ["Languages", "Other"],
  "activationEvents": ["onCommand:coffee2preview.check"],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "coffee2preview.check",
        "title": "Coffeescript check"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "coffeescript": "latest"
  }
}
